# Cloudflare KV Configuration for Dynamic Pricing
# These environment variables are used to fetch pricing data from Cloudflare KV at build time
# If not provided, the application will use default pricing data as fallback

# Cloudflare Account ID
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id

# Cloudflare API Key (not API token)
CLOUDFLARE_API_KEY=your_cloudflare_api_key

# Cloudflare Email (associated with the API key)
CLOUDFLARE_EMAIL=your_cloudflare_email

# KV Namespace ID for the pricelist
KV_PRICELIST_NAMESPACE_ID=your_kv_namespace_id

# Optional: Build-time environment
NODE_ENV=production